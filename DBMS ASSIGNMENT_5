1.SELECT PNO,PNAME FROM PARTS WHERE PRICE <20;

2.SELECT DISTINCT(PNO) FROM ODETAILS;

3.SELECT * FROM CUSTOMERS WHERE CNAME LIKE 'S%';

4.SELECT ORDERS.ONO,CUSTOMERS.CNAME FROM ORDERS,CUSTOMERS WHERE CUSTOMERS.CNO=ORDERS.CNO AND 

ORDERS.SHIPPED IS NULL;

5.SELECT C.CNAME,E.ENAME FROM EMPLOYEES E,CUSTOMERS C,ORDERS O WHERE O.CNO=C.CNO AND O.ENO=E.ENO;

6.SELECT ENAME FROM EMPLOYEES WHERE HDATE = (SELECT MIN(HDATE) FROM EMPLOYEES);

7.SELECT PNO,PNAME,PRICE FROM PARTS WHERE PRICE >20000 ORDER BY PNO;

8.

9.SELECT SUM(QTY) FROM ODETAILS WHERE PNO=10601;

10.SELECT E.ENO FROM EMPLOYEES E,ZIPCODES Z WHERE E.ZIP=Z.ZIP AND Z.CITY = 'MUMBAI';
