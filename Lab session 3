1. UPDATE PRODUCT_MASTER SET RECORDLVL = 80 WHERE DESCRIPTION like 'Shirts';
2. UPDATE PRODUCT_MASTER SET SELLPRICE=SELLPRICE*1.1;
3. UPDATE SALESMAN_MASTER SET YTDSALES = YTDSALES +35 WHERE SALESMANNO = 'S00003';
4. UPDATE SALES_ORDER_DETAILS SET QtyDisp = QtyOrdered WHERE ProductRate <= 1000;
5. UPDATE CLIENT_MASTER SET Pincode = 400001 WHERE City = 'Mumbai';
6. UPDATE PRODUCT_MASTER SET Recordlvl = 2 * Recordlvl;
7. DELETE FROM SALES_ORDER WHERE OrderStatus = 'cancelled';
8. DELETE FROM SALESMAN_MASTER;
9. SELECT * FROM PRODUCT_MASTER WHERE QtyInHand < ReorderLvl;
10.UPDATE EMP SET MGR = (SELECT EMPNO FROM EMP WHERE ENAME = 'Blake') WHERE ENAME IN ('Smith', 'Adam');
