1.
SELECT CITY FROM CUSTOMERS_74 UNION SELECT CITY FROM SUPPLIERS_74;

2.
SELECT CITY FROM CUSTOMERS_74 UNION ALL SELECT CITY FROM SUPPLIERS_74;

3.
SELECT  CITY FROM CUSTOMERS_74 WHERE CUSTOMERS_74.COUNTRY='Germany' UNION SELECT CITY FROM SUPPLIERS_74 WHERE SUPPLIERS_74.COUNTRY='Germany';

4.
SELECT CUSTOMERNAME, SUPPLIERNAME FROM CUSTOMERS_74, SUPPLIERS_74 WHERE SUPPLIERS_74.CITY IN(SELECT CITY FROM CUSTOMERS_74 INTERSECT SELECT CITY FROM SUPPLIERS_74) AND CUSTOMERS_74.CITY IN (SELECT CITY FROM CUSTOMERS_74 INTERSECT SELECT CITY FROM SUPPLIERS_74);

5.
SELECT COUNTRY FROM SUPPLIERS_74 WHERE COUNTRY IN(SELECT COUNTRY FROM SUPPLIERS_74 MINUS SELECT COUNTRY FROM CUSTOMERS_74);
