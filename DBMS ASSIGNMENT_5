CREATE TABLE ZIPCODES(
ZIP NUMBER(6) PRIMARY KEY,
CITY VARCHAR2(20)
);

CREATE TABLE EMPLOYEES(
ENO NUMBER(3) PRIMARY KEY,
ENAME VARCHAR2(15),
ZIP NUMBER(6) REFERENCES ZIPCODES(ZIP),
HDATE DATE
);

CREATE TABLE PARTS(
PNO NUMBER(3) PRIMARY KEY,
PNAME VARCHAR2(10),
QOH NUMBER(3),
PRICE NUMBER(4),
LVL NUMBER(2)
);

CREATE TABLE CUSTOMERS(
CNO NUMBER(3) PRIMARY KEY,
CNAME VARCHAR2(15),
STREET VARCHAR2(20),
ZIP NUMBER(6) REFERENCES ZIPCODES(ZIP),
PHONE NUMBER(10)
);

CREATE TABLE ORDERS(
ONO NUMBER(3) PRIMARY KEY,
CNO NUMBER(3) REFERENCES CUSTOMERS(CNO),
ENO NUMBER(3) REFERENCES EMPLOYEES(ENO),
PRECEIVED DATE,
SHIPPED DATE
);

CREATE TABLE ODETAILS(
ONO NUMBER(3) REFERENCES ORDERS(ONO),
PNO NUMBER(3) REFERENCES PARTS(PNO),
QTY NUMBER,
PRIMARY KEY(ONO,PNO)
);


