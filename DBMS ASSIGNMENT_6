CREATE TABLE CUSTOMERS_74(
CUSTOMERID NUMBER(1) PRIMARY KEY,
CUSTOMERNAME VARCHAR2(30),
CONTACTNAME VARCHAR2(30),
ADDRESS VARCHAR2(30),
CITY VARCHAR2(15),
POSTALCODE NUMBER(5),
COUNTRY VARCHAR2(10)
);

CREATE TABLE SUPPLIERS_74(
SUPPLIERID NUMBER(1) PRIMARY KEY,
SUPPLIERNAME VARCHAR2(30),
CONTACTNAME VARCHAR2(30),
ADDRESS VARCHAR2(30),
CITY VARCHAR2(15),
POSTALCODE NUMBER(5),
COUNTRY VARCHAR2(10)
);

